{
  "homepage": "http://vandercijr.github.io/oplabcaggregator",
  "name": "oplabcaggregator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "predeploy:client": "cd frontend && react-scripts build",
    "predeploy:server": "cd backend && babel --ignore node_modules,src/frontend/* --out-dir lib",
    "deploy": "gh-pages -d build",
    "build:client": "cd frontend && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:server": "cd backend && npm start",
    "start:client": "cd frontend && npm start",
    "start": "npm-run-all --parallel start:client start:server"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0"
  }
}
